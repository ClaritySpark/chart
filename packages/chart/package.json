{
  "name": "@clarityspark/chart",
  "version": "0.1.10",
  "description": "chart component for react",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "prepack": "pnpm build",
    "build": "pnpm run clean && tsup src --dts --format esm,cjs",
    "build:tsc": "tsc --emitDeclarationOnly",
    "build:js": "node build.js",
    "test": "jest",
    "clean": "rm -rf dist"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ClaritySpark/chart"
  },
  "keywords": [
    "chart UI",
    "react chart"
  ],
  "author": "<pmhxxhsj@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@clarityspark/shared": "workspace:*",
    "class-variance-authority": "0.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@jest/types": "^29.6.3",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^16.0.0",
    "@types/jest": "^29.5.12",
    "@types/node": "18.11.18",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vercel/style-guide": "5.2.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "tailwindcss": "3.4.4",
    "ts-jest": "^29.2.1",
    "ts-node": "^10.9.2",
    "typescript": "5.3.3"
  }
}
